package com.inhertance.value1;

abstract class Demo{
	private int a=20;
	public Demo() {
		
		System.out.println("this constructor excecuted by the c hild class");
	}
}
class Child extends Demo{
	Child(){
		System.out.println(a);
	}
}

public class TestClass1 {

	public static void main(String[] args) {
		Child ch=new Child();
		
		
	}

}
